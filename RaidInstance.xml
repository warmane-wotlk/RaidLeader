			<TileSize>
				<AbsValue val="32" />
			</TileSize>
			<EdgeSize>
				<AbsValue val="32" />
			</EdgeSize>
		</Backdrop>
		<Layers>
			<Layer level="BACKGROUND">
				<Texture name="$parent_Background" setAllPoints="true">
					<Anchors>
						<Anchor point="TOPLEFT">
							<Offset>
								<AbsDimension x="10" y="-10" />
							</Offset>
						</Anchor>
						<Anchor point="BOTTOMRIGHT">
							<Offset>
								<AbsDimension x="-10" y="10" />
							</Offset>
						</Anchor>
					</Anchors>
					<Color a="0.5" r="0" g="0" b="0" />
				</Texture>
			</Layer>
			<Layer level="OVERLAY">
				<Texture name="$parentHeaderRegion" file="Interface\DialogFrame\UI-DialogBox-Header">
					<Size>
						<AbsDimension x="300" y="64" />
					</Size>
					<Anchors>
						<Anchor point="TOP">
							<Offset>
								<AbsDimension x="0" y="12" />
							</Offset>
						</Anchor>
					</Anchors>
				</Texture>
				<FontString name="$parent_Header" inherits="GameFontHighlight" text="Title">
					<Size>
						<AbsDimension x="140" y="13" />
					</Size>
					<Anchors>
						<Anchor point="TOP">
							<Offset>
								<AbsDimension x="0" y="-2" />
							</Offset>
						</Anchor>
					</Anchors>
					<FontHeight>
						<AbsValue val="15" />
					</FontHeight>
					<Shadow>
						<offset>
							<AbsDimension x="2" y="-2" />
						</offset>
						<color r="0" g="0" b="0" />
					</Shadow>
				</FontString>
			</Layer>
		</Layers>
		<Scripts>
			<OnLoad>
				this:RegisterForDrag("LeftButton");
			</OnLoad>
			<OnDragStart>
				this:StartMoving();
				this.isMoving = true;
			</OnDragStart>
			<OnDragStop>
				this:StopMovingOrSizing();
				this.isMoving = false;
			</OnDragStop>
		</Scripts>
